; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="game_jam_9_avril"
run/main_scene="res://game/game.tscn"
config/icon="res://icon.png"

[autoload]

GlobalGame="*res://game/global_game.gd"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

2d/physics_engine="GodotPhysics"
2d/cell_size=16
2d/default_gravity_vector=Vector2( 0, 2 )

[rendering]

quality/filters/anisotropic_filter_level=1
quality/shading/force_vertex_shading=true
quality/shading/force_lambert_over_burley=true
quality/shading/force_blinn_over_ggx=true
quality/subsurface_scattering/quality=0
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
